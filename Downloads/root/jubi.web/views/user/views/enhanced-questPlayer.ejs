                            <ng-container ng-if="!usr.user.label">
                                <p class="user1"><span>{{usr.user.firstName}}{{usr.user.lastName}}</span><span><a href="#" ng-click="toggleUser(usr, false, true)"><img ng-if="!quest.complete" src="<%-config.web.contentUrl%>/img/blue-x.svg" /></a></span></p>
                            </ng-container>

                        </span>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 zeropadding" ng-show="appreciateShow" ng-if="quest.inspireAvailableToUser && shareUserArry.length == 1 && getCurrentBonusPointsAvailable()">
                        <input ng-model="inspireModel.bonusPoints" placeholder="Award Bonus Points" ng-disabled="quest.complete" ng-show="showBonus()" style="height: 32px;font-size: 12px;text-align: center;" />
                        <p class="haserror" ng-if="errArry.bonusPoints.max">Only {{errArry.bonusPoints.remain}} bonus points available to assign. </p>
                    </div> 
                </form>
            </div>
           
        </div>


    </div>



</div>