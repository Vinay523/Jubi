
<div>
    <div class="obj-challenges-container row" ng-show="inState=='start'" ng-controller="QuestPlayerStartController" ng-cloak>
        <div class="obj-challenges text-center">
            <img src="<%-config.web.contentUrl%>/img/target.png" />
            <p class="objective-heading">Objective</p>
            <!--<p class="obj-description">
                {{quest.objective}}
            </p>-->
            <ng-quill-editor ng-model="quest.objective" read-only="true"></ng-quill-editor>
            <div class="challenges text-center">
                <p>Challenges</p>
                <ng-container ng-repeat="c in quest.challenges">
                    <ng-container ng-if="c.type != 'finish'">
                        <ng-container ng-if="c.result">
                            <div class="challenge1finish">
                                <p ng-click="gotoChallenge($index)" style="cursor:pointer; z-index:1; position:relative" id="{{c.id}}"><span>{{$index + 1}}. {{c.title}}</span><img src="<%-config.web.contentUrl%>/img/greencheck.png" style="float:right;" /></p>
                            </div>
                        </ng-container>
                        <ng-container ng-if="!c.result">
                            <div class="challenge1">
                                <p>{{$index + 1}}. {{c.title}}</p>
                            </div>
                        </ng-container>
                    </ng-container>

                    <ng-container ng-if="c.type == 'finish'">
                        <ng-container ng-if="quest.challengesComplete != quest.challengeCount">
                            <div class="challenge1">
                                <p>{{$index + 1}}. {{c.title}}</p>
                            </div>
                        </ng-container>
                        <ng-container ng-if="quest.challengesComplete == quest.challengeCount">
                            <div class="challenge1finish">
                                <p ng-click="start()" style="cursor:pointer; z-index:1; position:relative" id="{{c.id}}">{{$index + 1}}. {{c.title}}<img src="<%-config.web.contentUrl%>/img/greencheck.png" style="float:right;" /></p>
                            </div>
                        </ng-container>
                    </ng-container>
                    

                </ng-container>
            </div>
        </div>
    </div>

    <div id="chal-section" class="content-bar row" ng-show="inState=='challenge'" ng-controller="QuestPlayerChallengeController" ng-cloak>
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="question col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="col-lg-7 col-lg-push-5 col-md-7 col-md-push-5 col-sm-12 col-xs-12 question-image text-center">
                    
                        <div ng-if="challenge.media.length > 0">
                            <%- partial('user/partials/challengeMedia') %>
                        </div>
                    
                </div>
                <div class="col-lg-5 col-lg-pull-7 col-md-5 col-md-pull-7 col-sm-12 col-xs-12 question-text">
                    <div ng-if="challenge.instructions" style="padding-top:20px;">
                        <!--<p class="instructions" ng-bind-html="toHtml(challenge.instructions)"></p>-->
                        <ng-quill-editor ng-model="challenge.instructions" read-only="true"></ng-quill-editor>
                        
                    </div>
                    <br />
                    <br />
                    
                    <form action="" method="post" name="challengeQuestions" ng-submit="submit($event,challenge)">
                        <div ng-if="challenge.questions.length > 0">
                            <div ng-if="challenge.questions.length > 0">
                                <div class="noTouch-response" ng-if="showUntouchedMsg">
                                    Please answer this question before submitting
                                </div>
                                <div ng-repeat="question in challenge.questions">
                                    <%- partial('user/partials/challengeQuestion') %>
                                </div>
                            </div>

                            <div class="success-response" ng-class="{ 'pulse' : val.correct }" ng-if="val.correct">
                                
                                <ng-container ng-if="!challenge.finishText">
                                    {{(challenge.finishText ? challenge.finishText : 'Correct!')}}
                                </ng-container>

                                <ng-container ng-if="challenge.finishText">
                                    <ng-quill-editor ng-model="challenge.finishText" read-only="true" ng-if="challenge.finishText"></ng-quill-editor>
                                </ng-container>
                               
                            </div>

                            <div class="failure-response" ng-class="{ 'pulse' : val.wrong }" ng-if="val.wrong">Oops! Try again!</div>
                            <ng-container ng-repeat="hint in hintsAry">
                                <div class="hint col-lg-12 col-md-12 col-sm-12 col-xs-12" ng-show="shownHint != null && shownHint >= {{$index}}">
                                    <p>Hint: {{hint.hint}}</p>
                                </div>
                            </ng-container>
                        </div>
                        
                        <!--<div class="success-response col-lg-6 col-md-6 col-sm-12 col-xs-12" ng-class="{ 'pulse' : val.correct }" ng-if="val.correct">
                            {{(challenge.finishText ? challenge.finishText : 'Correct!')}}
                        </div>-->
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                        </div>
                        <div class="step-action col-lg-6 col-md-6 col-sm-12 col-xs-12" style="margin-bottom:20px">
                            <!--<div class="check-wrong animated" ng-class="{ 'pulse' : val.wrong }" ng-if="val.wrong">Oops! Try again!</div>-->
                            <!--<div class="check-correct animated" ng-class="{ 'pulse' : val.correct }" ng-if="val.correct">{{(challenge.finishText ? challenge.finishText : 'Correct!')}}</div>-->
                            <button type="submit" id="challenge-formSubmit" class="btn btn-success hidden" ng-show="!canContinue">{{checkLabel(challenge)}}</button>
                            <!--<button ng-disabled="challengeQuestions.$dirty && challengeQuestions.touched" id="challenge-formIsHandled" class="btn btn-success hidden" ng-click="isChallengeTouched()"></button>-->
                            <!--<button type="button" class="btn btn-success" ng-click="continue(challenge)" ng-show="canContinue">Continue</button>-->
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                        </div>
                        
                        
                        
                    </form>
                    
                </div>
                
            </div>

        </div>
        <!--<div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
            <div class="failure-response" ng-class="{ 'pulse' : val.wrong }" ng-if="val.wrong">Oops! Try again!</div>
            <ng-container ng-repeat="hint in hintsAry">
                <div class="hint col-lg-6 col-md-6 col-sm-12 col-xs-12" ng-show="shownHint != null && shownHint >= {{$index}}">
                    Hint: {{hint.hint}}
                </div>
            </ng-container>
        </div>-->
    </div>

    <div class="congratulations-container row" ng-show="inState=='finish'" ng-controller="QuestPlayerFinishController" ng-cloak>
        <div class="congratulations-content text-center">
            <div>
                <p class="congrats" ng-if="!challenge.finishText">{{challenge.finishText ? challenge.finishText : 'CONGRATULATIONS'}}</p>
                <ng-container ng-if="challenge.finishText">
                    <ng-quill-editor ng-model="challenge.finishText" read-only="true"></ng-quill-editor>
                </ng-container>

                <p class="congrats-msg" ng-if="!challenge.instructions">{{challenge.instructions ? challenge.instructions : 'You have successfully completed the activity ' + quest.title}}</p>
                <ng-container ng-if="challenge.instructions">
                    <ng-quill-editor ng-model="challenge.instructions" read-only="true"></ng-quill-editor>
                </ng-container>
            </div>
            <div>
                <p class="just-earned">YOU JUST EARNED</p>
                <p class="earned-points">
                    <span>{{quest.score.points.earned ? quest.score.points.earned : '0'}}pts</span>
                </p>
                <p class="AND" ng-if="badge">
                    <span>AND</span>
                </p>
                <p class="completion-badge" ng-if="badge">
                    <span class="mask">
                        <span class="img">
                            <img ng-if="badge.imageUrl" ng-src="{{badge.imageUrl + '/100x100'}}" />
                            <img ng-if="!badge.imageUrl" src="/img/Badge Copy.png" />
                        </span>
                    </span>
                    <span class="title">{{badge.title}}</span>
                </p>
            </div>
            <ng-container ng-repeat="m in media">
                <div class="You-can-learn" ng-if="m.type==='link' && m.link">
                    You can learn more by visiting the following website.
                    <a ng-href="{{m.link}}" target="_blank">{{m.link}}</a>
                </div>
                <div class="Here-is-some-additio" ng-if="m.type==='resource' && m.url">
                    Here is some additional reading material for you.<br>
                    <a ng-href="{{m.url}}" target="_blank">{{m.name ? m.name : m.url}}</a>
                </div>
            </ng-container>

            <div class="Would-you-like-to-Up" ng-if="challenge.canUploadContent ? challenge.canUploadContent: false">
                Would you like to Upload Material others might find helpful?
                 
                <a href="" type="button" class="Upload-button" style="position: relative;">
                  
                    <img src="<%-config.web.contentUrl%>/img/upload-icon.svg"> Upload Material
                  
                </a>
                <input type="file" title="" ng-file-select ng-file-change="uploadFile($files)" ng-accept="'.pdf'" ng-disabled="upload.active" />
                <span ng-if="upload.active">
                    <i class="fa fa-spinner fa-spin"></i>
                    Uploading Your Material...
                </span>
                <p>8MB Max File Size</p>
                <p class="file-link" ng-if="uploadFileName">{{uploadFileName}}<!--<a class="close" ng-click="resetVal()"><img src="<%-config.web.contentUrl%>/img/blue-x.svg"></a>--></p>
                <div class="text-sm text-danger" ng-if="val.upload.type">Unsupported file type.</div>
                <div class="text-sm text-danger" ng-if="val.upload.size">File is too large.</div>
                <div class="text-sm text-success animated" ng-if="upload.status=='ok'" ng-class="{'fadeOut':upload.fade}">
                    <i class="fa fa-thumbs-o-up"></i> Thank you, your material has been uploaded.
                </div>
                <div class="text-sm text-danger animated" ng-if="upload.status=='err'" ng-class="{'fadeOut':upload.fade}">
                    <i class="fa fa-times"></i> Sorry, cannot upload your material at this time.
                </div>
            </div>
            <!--<hr ng-if="quest.finishers.length > 0">-->
            <!--<div class="participants" style="z-index: 0;position: relative;" ng-if="quest.finishers.length > 0">
                <p>
                    <ng-container ng-repeat="ava in quest.finishers">
                        <span class="participant1"><img ng-src="{{ava.avatarUrl + '/100x100'}}" /></span>
                    </ng-container>
                    <span class="participant4">{{quest.totalFinishers}}</span>
                </p>
                <p><span class="participant-count"></span> Participants completed this Challenge</p>
            </div>-->

        </div>
    </div>

    <div class="Do-content row" ng-show="inState=='do'" ng-controller="QuestPlayerDoController" ng-cloak >
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 doObj-description" ng-if="doQuest.objective != null">
            <p class="objectiveHeader">Objective</p>
            <!--<p class="objectiveSentence">{{doQuest.objective}}</p>-->
            <ng-quill-editor ng-model="doQuest.objective" read-only="true"></ng-quill-editor>
            
        </div>
        <form action="" method="post" id="toDoActivityQuestions" name="toDoActivityQuestions" ng-submit="doSubmit($event)">
            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 verification-section zeropadding">
                <!--<div class="Verification-Require col-lg-12 col-md-12 col-sm-12 col-xs-12 zeropadding">{{todoActivityChallenge.todo.verificationInstructions}}</div>-->
                <div class="Please-review-the-en col-lg-12 col-md-12 col-sm-12 col-xs-12 zeropadding">{{todoActivityChallenge.todo.validate ? 'Verification Required' : 'Honor System'}}</div>
                <div class="Please-review-the-inst-header col-lg-12 col-md-12 col-sm-12 col-xs-12 zeropadding">Instructions</div>
                <div class="Please-review-the-inst col-lg-12 col-md-12 col-sm-12 col-xs-12 zeropadding"><!--{{todoActivityChallenge.todo.instructions}}-->
                    <ng-quill-editor ng-model="todoActivityChallenge.todo.instructions" read-only="true"></ng-quill-editor>
                </div>
                <div class="duedate-calendar col-lg-12 col-md-12 col-sm-12 col-xs-12 zeropadding">
                    <!--<p>Due Date</p>
                    <input type="text" ng-model="currentDate" calendar  ng-if="userEnableDueDate"/>

                <div data-toggle="calendar" style="cursor:pointer;">
                    <img src="<%-config.web.contentUrl%>/img/calendar.png">
                </div>-->
                </div>
                <md-content layout-padding ng-cloak class="datePickerCal">

                    <div class="col-xs-4" layout-gt-xs="row">
                        <div class="datepickerCont" flex-gt-xs ng-if="userEnableDueDate">
                            <label>Due Date:</label>
                            <md-datepicker ng-model="currentDate" name="userDueDateField" md-min-date="minDueDate" md-placeholder="Enter date" ng-change="dateChanged(currentDate)"></md-datepicker>
                            <div ng-messages="toDoActivityQuestions.userDueDateField.$error">
                                <div class="hasError" ng-message="valid">The entered value is not a date!</div>
                                <div class="hasError" ng-message="required">This date is required!</div>
                                <div class="hasError" ng-message="mindate">Due Date cannot be same date or before.</div>
                                <div class="hasError" ng-if="dateIsError">Due Date is Empty.</div>
                                <!--<div ng-message="maxdate">Date is too late!</div>-->
                            </div>
                        </div>

                        <div class="datepickerCont" flex-gt-xs ng-if="!userEnableDueDate">
                            <label>Due Date:</label>
                            <md-datepicker ng-model="currentDate" md-placeholder="Enter date" disabled></md-datepicker>
                        </div>
                    </div>
                </md-content>

                <div class="Activity-Rectangle col-lg-12 col-md-12 col-sm-7 col-xs-7 zeropadding" ng-if="todoActivityChallenge.todo.resourceUrl">
                    <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 zeropadding">
                        <a ng-href="{{isResourceLinkFix(todoActivityChallenge.todo.resourceUrl)}}" target="_blank">
                            <img src="<%-config.web.contentUrl%>/img/document-Icon.png" />
                        </a>
                    </div>
                    <!--<div class="col-lg-10 col-md-10 col-sm-10 col-xs-10 zeropadding">  this is is resource is a array
                    <p class="Activity-Guidance">Activity Guidance</p>
                    <ng-container >
                        <p class="links">Engagement Challenges To-Do.docx</p>
                    </ng-container>
                    <p class="links">Engagement Challenges To-Do.docx</p>
                    <p class="links">www.learningstuff.com/engagement</p>
                    <p class="links">coolvideo.mov</p>
                </div>-->
                    <div class="col-lg-10 col-md-10 col-sm-10 col-xs-10 zeropadding">
                        <p class="Activity-Guidance">Activity Guidance</p>
                        <p class="links" ng-repeat="res in todoActivityChallenge.todo.resources"><a ng-href="{{isResourceLinkFix(res.url)}}" target="_blank">{{res.name}}</a></p>

                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 feedback-section">
                <div class=" add-verification">

                    <!--<p>{{todoActivityChallenge.todo.verificationInstructions}}</p>-->
                    <ng-quill-editor ng-model="todoActivityChallenge.todo.verificationInstructions" read-only="true"></ng-quill-editor>
                    <p ng-if="quest.userAllowedMediaUpload"><a class="verifyMedia" href="javascript:void(0)" ng-click="showDoAddMediaModalNotification()">ADD VERIFICATION</a></p>

                    <ng-container ng-repeat="media in challengeMedia">
                        <ng-container ng-if="media.type=='video' && media.source=='youtube'">
                            <div class="media-uploaded col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 media-image">
                                    <img ng-if="!media.coverUrl" class="media-icon" src="<%-config.web.contentUrl%>/img/youTube.png"  />
                                    <img ng-if="media.coverUrl" class="media-icon" src="" ng-src="{{media.coverUrl}}" />
                                    <div class="video-play">
                                        <i class="fa fa-play-circle-o" ng-click="playVideo(media)"></i>
                                    </div>
                                </div>
                                <div class="media-info col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                    <textarea class="media-link" placeholder="Media file">{{media.name}}</textarea>
                                    <textarea class="media-desc" placeholder="Description" ng-model="media.description">{{media.description}}</textarea>
                                </div>
                                <div class="close col-lg-1 col-md-1 col-sm-1 col-xs-1"><img ng-if="media.userId == currentUserId" ng-click="closeToDoClick(media)" src="<%-config.web.contentUrl%>/img/x.svg" /></div>
                            </div>
                        </ng-container>
                        <ng-container ng-if="media.type=='video' && media.source=='system'">
                            <div class="media-uploaded col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 media-image">
                                    <img ng-if="!media.coverUrl" class="media-icon" src="<%-config.web.contentUrl%>/img/media-placeholder.svg" />
                                    <img ng-if="media.coverUrl" class="media-icon" src="" ng-src="{{media.coverUrl}}" />
                                    <div class="video-play">
                                        <i class="fa fa-play-circle-o" ng-click="playVideo(media)"></i>
                                    </div>
                                    <div class="loading-overlay" ng-show="media.encodings.length <= 0" style="left:0px;right:20px;">
                                        <div class="loading loading-sm">
                                            <span us-spinner="{radius:9, width:4, length: 6, color: '#b0b0b7'}"></span>

                                            <div>encoding...</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="media-info col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                    <textarea class="media-link" placeholder="Media file">{{media.name}}</textarea>
                                    <textarea class="media-desc" placeholder="Description" ng-model="media.description">{{media.description}}</textarea>
                                </div>
                                <div class="close col-lg-1 col-md-1 col-sm-1 col-xs-1"><img ng-if="media.userId == currentUserId" ng-click="closeToDoClick(media)" src="<%-config.web.contentUrl%>/img/x.svg" /></div>
                            </div>
                        </ng-container>
                        <ng-container ng-if="media.type=='image'">
                            <div class="media-uploaded col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2 media-image">
                                    <a ng-href="{{media.url}}" target="_blank">
                                        <img class="thumbnail media" alt="" ng-src="{{media.url}}" />
                                    </a>
                                </div>
                                <div class="media-info col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                    <textarea class="media-link" placeholder="Media file">{{media.name}}</textarea>
                                    <textarea class="media-desc" placeholder="Description" ng-model="media.description">{{media.description}}</textarea>
                                </div>
                                <div class="close col-lg-1 col-md-1 col-sm-1 col-xs-1"><img ng-if="media.userId == currentUserId" ng-click="closeToDoClick(media)" src="<%-config.web.contentUrl%>/img/x.svg" /></div>
                            </div>
                        </ng-container>
                        <ng-container ng-if="media.type=='audio'">
                            <div class="media-uploaded col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                   <div class="col-lg-11 col-md-11 col-sm-11 col-xs-11 media-image audio-wrapper">
                                       <div class="audio_desc">
                                           <videogular vg-player-ready="playerReady(media, $API)" vg-plays-inline="'true'">
                                               <vg-media vg-type="audio" vg-src="media.sources" vg-native-controls="true"></vg-media>
                                               
                                           </videogular>
                                           <div class="audio-description" ng-if="media.description">{{media.description}}</div>
                                       </div>
                                    <div class="media-info audio-media">
                                        <textarea class="media-link" placeholder="Media file">{{media.name}}</textarea>
                                        <textarea class="media-desc" placeholder="Description" ng-model="media.description">{{media.description}}</textarea>
                                    </div>
                                </div>
                                <div class="close col-lg-1 col-md-1 col-sm-1 col-xs-1 zeropadding"><img ng-if="media.userId == currentUserId" ng-click="closeToDoClick(media)" src="<%-config.web.contentUrl%>/img/x.svg" /></div>
                            </div>
                        </ng-container>
