        <h4 class="modal-title">{{title}}</h4>
    </div>
    <!--<div class="modal-body">
        <span ng-repeat="name in headers"><label for="inputName" style="margin-right:10px;"><input type="checkbox" ng-model="name.selected" id="inputName" ng-change="captureUnselected(name)"/>{{name.name}}</label></span>
    </div>-->
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 modal-body">
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 exportModalFont" ng-repeat="name in customData">
            <label style="margin-right:10px;"><input type="checkbox" ng-model="name.selected" ng-change="changeExportOption()"/>{{name.name}}</label>
        </div>
        <!--<span ng-repeat="name in customData">
            <label style="margin-right:10px;"><input type="checkbox" ng-model="name.selected" />{{name.name}}</label>
        </span>-->
    </div>
    <div class="modal-footer">
        <button class="btn btn-primary btn-sm" ng-click="exportCustomTable(customData); ok()">{{okLabel}}</button>
        <button class="btn btn-primary btn-sm" ng-click="cancelModal()">Cancel</button>
    </div>
</script>